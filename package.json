{
  "name": "tempso-com-temp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack 2>&1 | grep -v 'baseline-browser-mapping.*over two months old' | tee /dev/stderr; exit ${PIPESTATUS[0]}",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 50",
    "type-check": "tsc --noEmit",
    "pre-push": "npm run type-check && npm run lint && npm run build",
    "check": "npm run lint && npm run type-check && npm run build",
    "prepare": "echo \"#!/bin/sh\\nnpm run pre-push\" > .git/hooks/pre-push && chmod +x .git/hooks/pre-push"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.16",
    "baseline-browser-mapping": "^2.8.32",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "postcss": "^8.4.31",
    "postcss-import": "^16.1.1",
    "postcss-preset-env": "^10.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "overrides": {
    "baseline-browser-mapping": "^2.8.32"
  }
}
